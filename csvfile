import pandas as pd

# Read the CSV file
df = pd.read_csv('path_to_your_csv_file.csv')

# Function to get unique environments for a given appid
def get_unique_environments(appid):
    filtered_df = df[df['appid'] == appid]
    unique_environments = filtered_df['environment'].unique().tolist()
    return unique_environments

# Function to get details for a given appid and environment
def get_app_details(appid, environment):
    filtered_df = df[(df['appid'] == appid) & (df['environment'] == environment)]
    return filtered_df.to_dict(orient='records')
